# Application mode (LOCAL, TEST, PROD, DEV)
MODE=PROD

# Service (API_V1, CLI)
SERVICE=API_V1

# Common project settings
PROJECT.NAME=app

# API settings
API.PORT=17000

# Database settings
DB.HOST=localhost
DB.PORT=17010
DB.USER=postgres
DB.PASSWORD=postgres
DB.NAME=postgres

# Redis settings
REDIS.HOST=localhost
