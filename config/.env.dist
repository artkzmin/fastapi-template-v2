LOGS.DIR_PATH=./logs
LOGS.MAX_LOG_FILES=10

# Security settings
SECURITY.HASH_KEY=supersecret_hash_key
SECURITY.ENCRYPT_KEY=supersecret_encrypt_key

# Proxy settings
PROXY.HOST=proxy.example.com
PROXY.PORT=1080
PROXY.USER=proxyuser
PROXY.PASSWORD=proxypass
